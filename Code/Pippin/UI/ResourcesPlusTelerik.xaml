                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border BorderBrush="{StaticResource OuterControlBorderBrush}" BorderThickness="1" CornerRadius="6">
                            <Border Margin="0" Background="{StaticResource ControlBackgroundBrush}" BorderBrush="{StaticResource InnerControlBorderBrush}" BorderThickness="1" CornerRadius="5" Padding="6">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="6"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="6"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>
                                    <telerikNavigation:RadToolBar Grid.Row="0" telerik:StyleManager.Theme="{StaticResource Theme}" Style="{StaticResource RadToolBarStyle_edp}">
                                        <telerik:RadButton Padding="2" Margin="0,2" Command="{Binding RefreshReportCommand}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/Refresh.png"/>
                                        </telerik:RadButton>
                                        <telerikNavigation:RadToolBarSeparator/>
                                        <telerik:RadButton Padding="2" Margin="0,2" Command="{Binding MoveToFirstPageCommand}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/FirstPage.png"/>
                                        </telerik:RadButton>
                                        <telerik:RadButton Padding="2" Margin="0,2" Command="{Binding MoveToPreviousPageCommand}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/PrevPage.png"/>
                                        </telerik:RadButton>
                                        <telerikNavigation:RadToolBarSeparator/>
                                        <TextBox IsEnabled="{Binding IsMoveToPageEnabled}" Margin="10, 0, 0, 0" MinWidth="30" VerticalAlignment="Center" Text="{Binding PageNumber, Mode=TwoWay}" TextAlignment="Center"/>
                                        <TextBlock Margin="2, 0, 10, 0" VerticalAlignment="Center" Text="{Binding PageCount, Converter={StaticResource FormatedNumberConverter}, ConverterParameter=of \{0\} pages}" TextAlignment="Center"/>
                                        <telerikNavigation:RadToolBarSeparator/>
                                        <telerik:RadButton Padding="2" Margin="0,2" Command="{Binding MoveToNextPageCommand}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/NextPage.png"/>
                                        </telerik:RadButton>
                                        <telerik:RadButton Padding="2" Margin="0,2" Command="{Binding MoveToLastPageCommand}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/LastPage.png"/>
                                        </telerik:RadButton>
                                        <telerikNavigation:RadToolBarSeparator/>
                                        <telerik:RadButton Padding="2" Margin="0,2" Command="{Binding PrintReportCommand}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/Print.png"/>
                                        </telerik:RadButton>
                                        <telerikInput:RadComboBox IsEnabled="{Binding IsExportEnabled}" Padding="2,0" Height="24" Margin="1,0" Width="45" DisplayMemberPath="LocalizedName" ItemsSource="{Binding RenderingExtensions}" SelectedItem="{Binding SelectedRenderingExtension, Mode=TwoWay}">
                                            <telerikInput:RadComboBox.SelectionBoxItemTemplate>
                                                <DataTemplate>
                                                    <Image Height="16" HorizontalAlignment="Left" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/Export.png"/>
                                                </DataTemplate>
                                            </telerikInput:RadComboBox.SelectionBoxItemTemplate>
                                        </telerikInput:RadComboBox>
                                        <telerikNavigation:RadToolBarSeparator/>
                                        <telerik:RadToggleButton Padding="2" Margin="0,2" Visibility="{Binding HasDocumentMap, Converter={StaticResource VisibilityConverter}}" IsChecked="{Binding IsDocumentMapVisible, Mode=TwoWay}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/DocumentMap.png"/>
                                        </telerik:RadToggleButton>
                                        <telerik:RadToggleButton Padding="2" Margin="0,2" Visibility="{Binding HasParameters, Converter={StaticResource VisibilityConverter}}" IsChecked="{Binding IsParametersAreaVisible, Mode=TwoWay}">
                                            <Image Height="16" Width="16" Source="/Telerik.ReportViewer.Silverlight;component/images/Parameters.png"/>
                                        </telerik:RadToggleButton>
                                    </telerikNavigation:RadToolBar>
                                    <Border x:Name="ParametersAreaPane" Visibility="{Binding IsParametersAreaVisible, Converter={StaticResource VisibilityConverter}}" Grid.Row="1">
                                        <Grid>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid Grid.Column="0">
                                                    <Telerik_ReportViewer_Silverlight:ReportParametersControl Foreground="{StaticResource ForegroundBrush}" ItemsSource="{Binding ParameterModels}" ParameterTemplateSelector="{StaticResource ReportParameterEditorTemplateSelector}">
                                                        <Telerik_ReportViewer_Silverlight:ReportParametersControl.ItemsPanel>
                                                            <ItemsPanelTemplate>
                                                                <Telerik_Windows_Controls_Primitives:RadUniformGrid Columns="2"/>
                                                            </ItemsPanelTemplate>
                                                        </Telerik_ReportViewer_Silverlight:ReportParametersControl.ItemsPanel>
                                                    </Telerik_ReportViewer_Silverlight:ReportParametersControl>
                                                </Grid>
                                                <Grid HorizontalAlignment="Center" VerticalAlignment="Bottom" Grid.Column="1">
                                                    <telerik:RadButton Padding="10, 3" Margin="3" Content="Preview" Command="{Binding ApplyReportParametersCommand}" telerik:StyleManager.Theme="{StaticResource Theme}"/>
                                                </Grid>
                                            </Grid>
                                            <Telerik_ReportViewer_Silverlight:DialogBox x:Name="UpdateParametersDialog" Visibility="Collapsed">
                                                <Grid Margin="20">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition/>
                                                        <RowDefinition Height="auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding UpdateParametersProgress}"/>
                                                    <telerik:RadProgressBar Style="{StaticResource ProgressBarStyle}" Grid.Row="1"/>
                                                </Grid>
                                            </Telerik_ReportViewer_Silverlight:DialogBox>
                                        </Grid>
                                    </Border>
                                    <Grid Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Border x:Name="DocumentMapPane" Width="200" Visibility="{Binding IsDocumentMapVisible, Converter={StaticResource VisibilityConverter}}" Grid.Column="0">
                                            <Border Margin="0, 0, 6, 0" Grid.Column="2" BorderBrush="{StaticResource OuterViewerAreaBorderBrush}" BorderThickness="1" CornerRadius="4">
                                                <Border Margin="0" Background="{StaticResource InnerViewerAreaBackground}" BorderBrush="{StaticResource InnerViewerAreaBorderBrush}" BorderThickness="1" CornerRadius="3">
                                                    <telerikNavigation:RadTreeView x:Name="DocumentMapTreeView" Background="{StaticResource InnerViewerAreaBackground}" IsEnabled="{Binding IsMoveToPageEnabled}" ItemsSource="{Binding DocumentMapNodes}" telerik:StyleManager.Theme="{StaticResource Theme}" SelectedItem="{Binding SelectedDocumentMapNode, Mode=TwoWay}">
                                                        <telerikNavigation:RadTreeView.ItemTemplate>
                                                            <telerik:HierarchicalDataTemplate ItemsSource="{Binding Children}">
                                                                <TextBlock Text="{Binding Label}"/>
                                                            </telerik:HierarchicalDataTemplate>
                                                        </telerikNavigation:RadTreeView.ItemTemplate>
                                                    </telerikNavigation:RadTreeView>
                                                </Border>
                                            </Border>
                                        </Border>
                                        <Border Grid.Column="2" BorderBrush="{StaticResource OuterViewerAreaBorderBrush}" BorderThickness="1" CornerRadius="4">
                                            <Border Margin="0" Background="{StaticResource InnerViewerAreaBackground}" BorderBrush="{StaticResource InnerViewerAreaBorderBrush}" BorderThickness="1" CornerRadius="3">
                                                <Grid>
                                                    <ScrollViewer x:Name="PageScrollViewer" BorderThickness="0" Visibility="Visible" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" telerik:ScrollViewerExtensions.EnableMouseWheel="True">
                                                        <Border x:Name="PageBorder" Height="{Binding PageHeight}" Margin="10" Width="{Binding PageWidth}" Background="{StaticResource PageBorderBackground}" BorderBrush="{StaticResource PageBorderBorderBrush}" BorderThickness="1">
                                                            <Telerik_Windows_Controls_Primitives:LayoutTransformControl x:Name="PageContainer" Content="{Binding PageRoot}" LayoutTransform="{Binding PageTransform}"/>
                                                        </Border>
                                                    </ScrollViewer>
                                                    <Telerik_ReportViewer_Silverlight:DialogBox x:Name="ViewerProgressMessage" Visibility="Collapsed">
                                                        <Grid Margin="20">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition/>
                                                                <RowDefinition Height="auto"/>
                                                            </Grid.RowDefinitions>
                                                            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding ProgressMessage}"/>
                                                            <telerik:RadProgressBar Style="{StaticResource ProgressBarStyle}" Grid.Row="1"/>
                                                        </Grid>
                                                    </Telerik_ReportViewer_Silverlight:DialogBox>
                                                    <TextBlock x:Name="ViewerError" HorizontalAlignment="Center" VerticalAlignment="Top" Visibility="Collapsed" FontFamily="Verdana" Foreground="{StaticResource ErrorMessageForeground}" Padding="20" Text="{Binding Error}"/>
                                                    <Telerik_ReportViewer_Silverlight:DialogBox x:Name="ExportDialog" Visibility="Collapsed">
                                                        <Grid Width="270">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="100"/>
                                                                <RowDefinition Height="auto"/>
                                                            </Grid.RowDefinitions>
                                                            <Grid HorizontalAlignment="Stretch" Margin="20" VerticalAlignment="Center">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition/>
                                                                    <RowDefinition Height="auto"/>
                                                                </Grid.RowDefinitions>
                                                                <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding ExportProgress}"/>
                                                                <telerik:RadProgressBar x:Name="ExportProgressBar" Style="{StaticResource ProgressBarStyle}" Visibility="Collapsed" Grid.Row="1"/>
                                                            </Grid>
                                                            <Grid Grid.Row="1">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="*"/>
                                                                    <ColumnDefinition Width="auto"/>
                                                                    <ColumnDefinition Width="auto"/>
                                                                </Grid.ColumnDefinitions>
                                                                <telerik:RadButton Padding="10, 3" Margin="5" Content="Save" Grid.Column="1" Command="{Binding SaveExportCommand}" telerik:StyleManager.Theme="{StaticResource Theme}"/>
                                                                <telerik:RadButton Padding="10, 3" Margin="5" Content="Cancel" Grid.Column="2" Command="{Binding CancelExportCommand}" telerik:StyleManager.Theme="{StaticResource Theme}"/>
                                                            </Grid>
                                                        </Grid>
                                                    </Telerik_ReportViewer_Silverlight:DialogBox>
                                                </Grid>
                                            </Border>
                                        </Border>
                                    </Grid>
                                    <Grid Grid.Row="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="auto"/>
                                        </Grid.ColumnDefinitions>
                                        <StackPanel VerticalAlignment="Center" Grid.Column="1" Orientation="Horizontal">
                                            <telerik:RadSlider VerticalAlignment="Center" Width="200" Maximum="100" Minimum="-100" SmallChange="10" Value="{Binding Zoom, Converter={StaticResource ScaleFactorConverter}, Mode=TwoWay}" telerik:StyleManager.Theme="{StaticResource Theme}" HandlesVisibility="Visible"/>
                                            <TextBlock HorizontalAlignment="Center" Margin="10, 0, 3, 0" VerticalAlignment="Center" Width="40" Foreground="{StaticResource ForegroundBrush}" Text="{Binding Zoom, Converter={StaticResource ZoomConverter}}"/>
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                            </Border>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>
