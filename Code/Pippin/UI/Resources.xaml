									<GradientStop Color="#FFFFFFFF" Offset="1"/>
									
								</LinearGradientBrush>
							</Border.Background>
							<ScrollViewer x:Name="ScrollViewer" BorderThickness="0" Padding="1">
								<ItemsPresenter/>
							</ScrollViewer>
						</Border>
					</Popup>
				</Grid>
			</ControlTemplate>
		</Setter.Value>
	</Setter>
</Style>


